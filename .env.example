# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_NAME=mycomp_db
DB_USER=mycomp_user
DB_PASS=mycomp_pass123

# 애플리케이션 설정
APP_ENV=development
APP_DEBUG=true
APP_TIMEZONE=Asia/Seoul

