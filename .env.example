CSRF_SECRET=your_csrf_secret_key_here
SESSION_LIFETIME=7200

# LOC 스캐너 설정
CPP_ENGINE_PATH=./cpp_engine/build/loc_scanner_engine
DEFAULT_SCAN_ENGINE=hybrid
MAX_SCAN_TIMEOUT=300